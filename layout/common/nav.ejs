<nav class="nav" role="navigation">
    <ul class="menu-list">
        <% for (let i in theme.menu) { %>
            <li class="menu-item">
                <a class="<%- is_current(theme.menu[i]) ? 'current':'' %>" href="<%- url_for(theme.menu[i]) %>"><%= __(i.toLowerCase()) %></a>
            </li>
        <% } %>
    </ul>
</nav>